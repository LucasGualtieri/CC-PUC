{
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: gcc-11 build active file",
			"command": "/usr/bin/gcc-11",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"${file}",
				"-I/home/lucas/PC-HOME/CC-PUC/Bibliotecas",
				"-o",
				"/home/lucas/PC-HOME/OutputExe/${fileBasenameNoExtension}.exe" // .exe optional
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": ["$gcc"],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "Task generated by Debugger."
		}
	],
	"version": "2.0.0"
}
